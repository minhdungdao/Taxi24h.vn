{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/dich-vu-khac.html",
        "dest": "/public/dich-vu-khac.html"
      },
      {
        "src": "/(.*)",
        "dest": "/app.js"
      }
    ]
  }
  